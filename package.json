{
  "family": "arale",
  "name": "calendar",
  "version": "0.9.0",
  "description": "Calendar widget.",
  "author": "Hsiaoming Yang <lepture@me.com>",
  "homepage": "http://aralejs.org/calendar/",
  "repository": {
    "type": "git",
    "url": "https://github.com/aralejs/calendar.git"
  },
  "bugs": {
    "url": "https://github.com/aralejs/calendar/issues"
  },
  "keywords": ["widget", "month"],

  "spm": {
    "alias": {
      "$": "$",
      "moment": "gallery/moment/2.0.0/moment",
      "handlebars": "gallery/handlebars/1.0.1/runtime",
      "base": "arale/base/1.0.1/base",
      "overlay": "arale/overlay/1.0.1/overlay",
      "widget": "arale/widget/1.0.3/widget"
    },
    "devAlias": {
      "mocha": "gallery/mocha/1.9.0/mocha",
      "expect": "gallery/expect/0.2.0/expect",
      "sinon": "gallery/sinon/1.6.0/sinon"
    },
    "engines": {
      "seajs": "seajs/seajs/2.0.0/sea"
    },
    "output": [
      "calendar.js",
      "date-column.js", "month-column.js", "year-column.js",
      "css/date.css", "css/month.css", "css/year.css",
      "i18n/*"
    ]
  }
}
